{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "off",
        "no-console": "off",
        "no-undef": "off",
        "no-promise-executor-return": "off",
        "no-prototype-builtins": "off",
        "import/extensions": "off",
        "max-len": "off"
    }
}
